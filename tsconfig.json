{
  "extends": "astro/tsconfigs/strict",
  "exclude": [
    "node_modules/*"
  ],
  "compilerOptions": {
    // "types": ["vite-plugin-pwa/info"]
    "paths": {
      "@components/*": ["src/components/*"],
      "@data/*": ["src/data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@scripts/*": ["src/scripts/*"],
      "@templates/*": ["src/templates/*"],
    }
  }
}