---
  // import SetCard from "../components/SetCard.svelte";
  import NewVerticalSetCard from "../components/NewVerticalSetCard.astro";
  import type { IData, ISetV2 } from "../scripts/types";

  interface Props {
    data: IData
    sets: ISetV2[]
    size?: number
  }
  const { sets, data, size = 128 } = Astro.props
---

<div class=" relative text-5xl sm:text-7xl w-full text-center my-5">{data.name}</div>

{sets.map((set) => (
      <NewVerticalSetCard set={set} data={data} size={size}/>
))}
